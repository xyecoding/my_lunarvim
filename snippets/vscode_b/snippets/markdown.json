    "description": "DIARY: FROM TIME"
  },
  "sdtm": {
    "prefix": "sdtm",
    "body": [
      "<font color=red>[DONE ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}]</font>"
